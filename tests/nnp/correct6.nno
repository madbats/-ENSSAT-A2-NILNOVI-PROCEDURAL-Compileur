procedure pp is
  procedure p(v : in integer; i : in out integer) is
  begin
    i := i + v
  end;
  procedure q(v : in integer; i : in out integer) is
  begin
    i := i * v
  end;
  a : integer;
begin
  a := 5;
  p(2,a);
  put(a);
  q(2,a);
  put(a)
end.
